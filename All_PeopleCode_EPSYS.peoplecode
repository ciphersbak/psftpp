&msgLog_.println_to_stdout("******************************************************************");
&msgLog_.println_to_stdout("Total Number of RowsAffected for Currency " | &ANZStateRec_.CURRENCY_CD.Value | " are " | &ANZTotalRowCount_);
&ANZStagingSQL_.Close();


