/* Create the error file in the following path */
SQLExec("SELECT PRCSOUTPUTDIR FROM PSPRCSPARMS WHERE PRCSINSTANCE = :1", &ANZStateRec_.PROCESS_INST_STG.Value, &ANZFileDir_);
