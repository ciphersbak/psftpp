      Else
         &Z_INV_DMD.Insert();
      End-If;
      rem &Imp_Parms_Upd = "";
      &LVL0REC.Z_SHIP_ONLY_FLG.Enabled = False;
   End-For;
End-If;
For &t = 1 To &Arr_ITEM.Len
   &Arr_ITEM.Pop();
End-For;

