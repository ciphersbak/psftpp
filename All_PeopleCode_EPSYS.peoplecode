   &mynotifyto.Push(&mynotifyaddress);
   &mynotifytemplate = create PT_WF_NOTIFICATION:NotificationTemplate("", "", "Z_RMA_DATA_ERR_1", "G");
   
   
   /* Populate an array to contain the values needed by the template */
