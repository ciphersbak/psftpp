   &GridColumn = &Grid.GetColumn("MERCHANT_GRIDTAB");
   If &RequiredFieldCountArray [4] > 0 Then
      &GridColumn.Label = "*" | MsgGetText(10504, 170, "Merchant");
   Else
      &GridColumn.Label = MsgGetText(10504, 170, "Merchant");
