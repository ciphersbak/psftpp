                           &BOOL_INS_VAL_N = &INSERT_NOFLG_SQL.Execute(&STRSheetID, &NBR_Orig_Line_nbr, 0, &STRActionPlan);
                           MessageBox(0, "", 0, 0, "&BOOL_INS_VAL_N " | &BOOL_INS_VAL_N);
