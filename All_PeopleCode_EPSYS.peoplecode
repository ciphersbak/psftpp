                           /* User is modifying the existing row(s) */
                           Local boolean &BOOL_UPD_VAL_N = &Update_NOFLG_SQL.Execute(&STRActionPlan, &STRSheetID, &NBR_Orig_Line_nbr);
                           MessageBox(0, "", 0, 0, "&BOOL_UPD_VAL_N " | &BOOL_UPD_VAL_N);
