      &z_req_worklist.Insert();
      &update = &z_wl_requisition.Update();
      
      If &update = 1 Then
         /* UPDATE SUCCESS */
         /* SAVE WL entry to DB */
         &save = &z_wl_requisition.Save();
         
         If &save <> 0 Then
