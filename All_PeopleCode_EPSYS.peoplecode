	        BEGIN
	                INSERT INTO @temp_table EXEC sp_spaceused @table_name, false ;
	        END
	        
	        FETCH NEXT FROM c1 INTO @table_name; 
	END; 
	CLOSE c1; 
	DEALLOCATE c1; 
	SELECT t1.*
	,       t2.schemaname 
	FROM @temp_table t1 
	INNER JOIN @tab1 t2 ON (t1.tablename = t2.tablename )
	ORDER BY  schemaname,tablename;
